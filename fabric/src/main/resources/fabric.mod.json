{
  "schemaVersion": 1,
  "id": "hexxyskies",
  "version": "${version}",
  "name": "Hexxy Skies",
  "description": "Hexcasting x Valkyrien Skies Interop",
  "authors": [
    "TechTastic",
    "walksanator"
  ],
  "contact": {
    "homepage": "https://techtastic.github.io/hexsky/v/latest/master/en_us/",
    "sources": "https://github.com/TechTastic/hexsky",
    "issues": "https://github.com/TechTastic/hexsky/issues"
  },
  "license": "MIT",
  "icon": "assets/hexxyskies/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.techtastic.hexxyskies.fabric.HexxySkiesFabric"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.github.techtastic.hexxyskies.fabric.client.HexxySkiesFabricClient"
      }
    ]
  },
  "mixins": [
    "hexxyskies.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.0",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=${architectury_api_version}",
    "fabric-api": "*",
    "valkyrienskies": ">=${vs2_version}",
    "hexcasting": ">=${hexcasting_version}"
  },
  "suggests": {
    "moreiotas": ">=${moreiotas_version}",
    "complexhex": ">=${complexhex_version}",
    "hexal": ">=${hexal_version}",
    "hexodus": ">=${hexodus_version}",
    "hexical": ">=${hexical_version}"
  },
  "breaks": {
    "hexsky": "*",
    "moreiotas": "<${moreiotas_version}",
    "hexal": "<${hexal_version}",
    "hexodus": "<${hexodus_version}",
    "hexical": "<${hexical_version}"
  }
}
